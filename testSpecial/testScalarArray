pvget -r value intArrayASL1
pvput -r  longArrayASL0 [1,2,3,4]
pvget -r value longArrayASL0
pvput -r  longArrayASL1 [1,2,3,4]
pvget -r value longArrayASL1
pvput -r  ubyteArrayASL0 [1,2,3,4]
pvget -r value ubyteArrayASL0
pvput -r  ubyteArrayASL1 [1,2,3,4]
pvget -r value ubyteArrayASL1
pvput -r  ushortArrayASL0 [1,2,3,4]
pvget -r value ushortArrayASL0
